{
  "name": "cluster-cms",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "engines": {
    "node": "13.6.0",
    "yarn": "1.22.0"
  },
  "scripts": {
    "start": "node app.js",
    "start_local": "node -r dotenv/config app.js",
    "server": "nodemon app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "build": "yarn --cwd frontend install && yarn --cwd frontend build:css && yarn --cwd frontend build",
    "deploy": "cp -a frontend/build/. public/",
    "heroku-postbuild": "yarn build && yarn deploy"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/error-reporting": "^1.1.3",
    "@google-cloud/tasks": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^39.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.9.4",
    "nodemailer": "^6.4.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  }
}
